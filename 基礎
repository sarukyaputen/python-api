    data = [{'a':"A",'b':(2,4),'c':3.0}]
    b = json.dumps(data)  # list è½‰ json
#................................................
    pd.json(data)
    b.to_csv('f:\out1.csv')
    
#---------------------------------------------------

import csv
with open("test.csv","w") as csvfile:
    writer = csv.writer(csvfile)
    
writer.writerow(["index","a_name","b_name"])
writer.writerows([[0,1,3],[1,2,3],[2,3,4]])
